{
    "name": "rollup-builder",
    "version": "1.3.0",
    "author": "Rajesh",
    "license": "MIT",
    "homepage": "https://github.com/Rajeshr34/rollup-builder",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Rajeshr34/rollup-builder.git"
    },
    "bugs": {
        "url": "https://github.com/Rajeshr34/rollup-builder/issues"
    },
    "keywords": [
        "typescript",
        "bundel",
        "rollup"
    ],
    "scripts": {
        "prepare": "husky install",
        "prettier:fix": "prettier --write",
        "prettier:check": "prettier --check src",
        "lint": "eslint --fix src",
        "build": "yarn lint && rimraf dist && tsc -p tsconfig.json && yarn pkts",
        "pkts": "ts-node ./package.json.ts"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "*.{tsx,ts,js,css,md}": [
            "yarn prettier:fix",
            "yarn lint"
        ]
    },
    "peerDependencies": {},
    "dependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@rollup/plugin-replace": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "eslint": "^8.1.0",
        "fs-extra": "^10.0.0",
        "lodash-es": "^4.17.21",
        "prettier": "^2.4.1",
        "progress-estimator": "^0.3.0",
        "rollup": "^2.59.0",
        "rollup-plugin-folder-input": "^1.0.1",
        "rollup-plugin-progress": "^1.1.2",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-ts": "^1.4.7",
        "ora": "^6.0.1",
        "chalk": "^4.1.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/lodash-es": "^4.17.5",
        "@types/node": "^16.11.6",
        "@types/rollup-plugin-progress": "^1.1.1",
        "@types/sade": "^1.7.3",
        "husky": "^7.0.4",
        "lint-staged": "^11.2.6",
        "lodash": "^4.17.21",
        "rimraf": "^3.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}
